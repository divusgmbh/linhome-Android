
## Start of default rc

[sip]
contact="Linphone Android" <sip:linphone.android@unknown-host>
use_info=0
keepalive_period=30000
sip_port=-1
sip_tcp_port=-1
sip_tls_port=-1
media_encryption=srtp

[net]
#Because dynamic bitrate adaption can increase bitrate, we must allow "no limit"
download_bw=0
upload_bw=0

[video]
size=vga

[app]
tunnel=disabled
push_notification=1
auto_start=1
debug=1

[tunnel]
host=
port=443

[misc]
version_check_url_root=https://sip.divus.eu/linhomereleases
log_collection_upload_server_url=https://sip.divus.eu/logs
history_max_size=100


## End of default rc
